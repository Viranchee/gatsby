{
  "parser": "@typescript-eslint/parser",
  "extends": [ "prettier/@typescript-eslint", "prettier/react", "plugin:prettier/recommended"],

  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "settings": {
    "react": {
      "version": "detect"
     }
  }
}
